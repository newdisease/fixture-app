    "react": "^18.2.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.7.0",
    "remix-auth-google": "^2.0.0",
    "remix-auth-totp": "^3.4.2",
