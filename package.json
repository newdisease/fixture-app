{
  "name": "fixture-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "format": "prettier --write .",
    "lint": "eslint .",
    "start": "remix-serve ./build/server/index.js",
    "test": "vitest",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "reinstall": "rm -rf node_modules && rm package-lock.json && npm install && npm run dev",
    "typecheck": "tsc"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/playwright-report",
    "/server-build"
